{
  "name": "transformers-router",
  "version": "1.0.0",
  "description": "A TypeScript router library for transformers",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint 'src/**/*.ts' --ignore-pattern 'src/**/*.test.ts'",
    "lint:fix": "eslint 'src/**/*.ts' --ignore-pattern 'src/**/*.test.ts' --fix",
    "prepare": "npm run build"
  },
  "keywords": [
    "transformers",
    "router",
    "typescript",
    "library"
  ],
  "author": "Kacper Paczos",
  "license": "MIT",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.7.1",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "eslint": "^9.37.0",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.5",
    "typescript": "^5.9.3"
  }
}
